{
  "name": "@akameco/sum",
  "version": "0.1.0",
  "description": "sum",
  "license": "MIT",
  "repository": "akameco/sum",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{ts,js,json,md}'",
    "type-check": "tsc --noEmit",
    "build": "NODE_ENV=production babel src -d lib --extensions '.ts,.tsx'",
    "watch": "tsc -p . --watch",
    "test": "jest",
    "test:cov": "jest --coverage --ci --runInBand src",
    "test:ci": "yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tsc --noEmit",
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.7",
    "all-contributors-cli": "^5.4.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
